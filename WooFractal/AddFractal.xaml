        </Button>
        <Button Height="58" HorizontalAlignment="Left" Margin="80,10,0,0" Name="button2" VerticalAlignment="Top" Width="58" Click="button2_Click">
            <StackPanel>
                <Image Source="/WooFractal;component/mengerthumb.jpg" />
            </StackPanel>
        </Button>
        <Button Height="58" HorizontalAlignment="Left" Margin="148,10,0,0" Name="button3" VerticalAlignment="Top" Width="58" Click="button3_Click">
            <StackPanel>
                <Image Source="/WooFractal;component/tetrathumb.jpg" />
            </StackPanel>
        </Button>
    </Grid>
</UserControl>
